{
    "name": "freezewarp/crawler",
    "type": "project",
    "description": "Minimal PHP crawler libraries.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "ext-http": "*",
        "ext-zlib": "*",
        "elasticsearch/elasticsearch": "^7.0",
        "guzzlehttp/guzzle": "^6.5",
        "symfony/dom-crawler": "*",
        "symfony/panther": "^1.0",
        "illuminate/support" : "^6",
        "yosymfony/toml": "^1.0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "JTP\\Crawler\\": "crawler/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
